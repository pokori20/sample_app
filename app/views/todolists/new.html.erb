<h1>新規投稿</h1>
<!--for_with部分-->
<%= form_with model:@list,url:'/todolists',local:true do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :body %>
  <!--画像用fieldの追加-->
  <h4>画像</h4>
  <%= f.attachment_field:image %>
  <!--投稿機能の追加-->
  <%= f.submit '投稿' %>
<% end %>
